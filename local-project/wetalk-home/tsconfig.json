{
    "compilerOptions": {
        "jsx": "preserve",
        "target": "ES2018",
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "experimentalDecorators": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"],
            "@/*": ["./*"]
        },
        "types": [
            "@nuxt/types",
            "@nuxtjs/axios",
            "@nuxtjs/i18n",
            "@types/crypto-js",
            "cookie-universal-nuxt",
            "@types/node",
            "@pinia/nuxt",
            "jquery",
            "gsap"
        ]
    },
    "vueCompilerOptions": {
        "target": 2.7
    },
    "exclude": ["node_modules", ".nuxt", "dist"]
}
